{
    "name": "secloud-integrator",
    "displayName": "secloud-integrator",
    "description": "Front end for SeCloud demonstrators ",
    "version": "0.0.1",
    "publisher": "pochi",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "configurarion": {},
    "main": "./client/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "secloud.runMLModelRecommender",
                "title": "SeCloud: Suggest sources and sinks"
            },
            {
                "command": "secloud.executeIFCMonitor",
                "title": "SeCloud: Execute IFC monitor"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "acorn": "5.7.1",
        "escodegen": "1.11.0",
        "estraverse": "4.2.0",
        "guardia-ifc": "^0.4.0",
        "node-fetch": "2.3.0",
        "vscode-languageclient": "^5.1.1",
        "vscode-languageserver": "^5.1.0",
        "vscode-uri": "^1.0.6"
    }
}
